import java.util.*;

public class javabasics {
    public static void main(String args[]){
         Scanner sc =new Scanner (System.in);
         int age = 11;
         if (age>=18){
          System.out.println("adult");
         }
         else if(age>13 && age<18) {
          System.out.println("teenager");
         }
         else {
          System.out.println("child");
         }
     }
}
